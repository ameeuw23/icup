{% trans_default_domain "admin" %}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>Generated e-mail: request for more information</title>
</head>
<body>
    <p>{% trans %}FORM.INFOEMAIL.TITLE{% endtrans %}:</p>
    <table cellpadding="0" cellspacing="0" border="0" width="100%">
        <tr>
            <td colspan="2" bgcolor="#225585" height="25" width="9"></td>
            <td valign="middle" bgcolor="#225585" height="25">
                <table cellpadding="0" cellspacing="0" border="0">
                <tr>
                <td style="font-family:Verdana, Geneva, sans-serif; font-size:12px; color:#ffffff" height="25">{% trans %}FORM.INFOEMAIL.SENDER{% endtrans %}</td>
                </tr>
                </table>
            </td>
            <td colspan="2" bgcolor="#225585" height="25" width="9"></td>
        </tr>
        <tr>
            <td bgcolor="#225585" width="1"></td><td width="8"></td><td valign="top">
                <table cellpadding="0" cellspacing="0" border="0">
                    <tr>
                        <td height="8" style="font-size:1px;line-height:1px" colspan="2"></td>
                    </tr>
                    <tr>
                        <td><strong>{% trans %}FORM.INFOEMAIL.CLUB{% endtrans %}:</strong></td><td><strong>{{ club }}</strong></td>
                    </tr>
                    <tr>
                        <td>{% trans %}FORM.INFOEMAIL.NAME{% endtrans %}:</td><td>{{ name }}</td>
                    </tr>
                    <tr>
                        <td height="7" style="font-size:1px;line-height:1px" colspan="2"></td>
                    </tr>
                    <tr>
                        <td>{% trans %}FORM.INFOEMAIL.EMAIL{% endtrans %}:</td><td>{{ email }}</td>
                    </tr>
                    <tr>
                        <td>{% trans %}FORM.INFOEMAIL.PHONE{% endtrans %}:</td><td>{{ phone }}</td>
                    </tr>
                    <tr>
                        <td height="7" style="font-size:1px;line-height:1px" colspan="2"></td>
                    </tr>
                    <tr>
                        <td colspan="2">{{ msg }}</td>
                    </tr>
                    <tr>
                        <td height="7" style="font-size:1px;line-height:1px" colspan="2"></td>
                    </tr>
                </table>
            </td><td width="8"></td><td bgcolor="#225585" width="1"></td>
        </tr>
        <tr>
            <td style="font-size:1px;line-height:1px" colspan="5" bgcolor="#225585" height="1" width="100%"></td>
        </tr>
    </table>
    <p>{% trans %}FORM.INFOEMAIL.REGARDS{% endtrans %}<br>icup support</p>
</body>
</html>
